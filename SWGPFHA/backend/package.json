{
  "name": "swgpfha-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:env": "cross-env NODE_ENV=development dotenv -e .env.development --",
    "prod:env": "cross-env NODE_ENV=production dotenv -e .env.production --",
    "dev:db:generate": "npx prisma generate --schema=prisma/dev.schema.prisma",
    "dev:db:migrate": "npx prisma migrate dev -n init_opportunities --schema=prisma/dev.schema.prisma",
    "dev:db:reset": "npx prisma migrate reset -f --schema=prisma/dev.schema.prisma",
    "prod:db:generate": "npx prisma generate --schema=prisma/prod.schema.prisma",
    "prod:db:migrate": "npx prisma migrate deploy --schema=prisma/prod.schema.prisma",
    "dev": "cross-env NODE_ENV=development dotenv -e .env.development -- tsx watch src/index.ts",
    "start": "cross-env NODE_ENV=production dotenv -e .env.production -- tsx src/index.ts",
    "postinstall": "npx prisma generate --schema=prisma/dev.schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "undici": "^7.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/express-serve-static-core": "^4.19.6",
    "@types/mime-types": "^3.0.1",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.30",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "express": "^4.21.2",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2"
  }
}
